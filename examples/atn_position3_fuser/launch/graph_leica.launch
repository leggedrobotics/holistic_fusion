<?xml version="1.0" encoding="UTF-8"?> 
<launch>
    <!-- Parameters -->
    <param name="use_sim_time"            value="true"/>

    <!-- Input Topics -->
    <arg name="imu_topic_name"            default="/gt_box/ap20/imu" />
    <arg name="position_topic_name"       default="/gt_box/ap20/prism_position" />

    <!-- Graph -->
    <include file="$(find atn_position3_fuser)/launch/graph_no_urdf.launch" pass_all_args="true"/>

    <!-- Static TF Publisher -->
    <node pkg="tf2_ros" type="static_transform_publisher" name="body_to_prism_tf_publisher" args="0.05513 -0.01502 -0.1503 0 0 0 body Prism" />
    <node pkg="tf2_ros" type="static_transform_publisher" name="body_to_imu_tf_publisher" args="0.0 0.0 0.0 1.5708 0 0 body imu" />
</launch>